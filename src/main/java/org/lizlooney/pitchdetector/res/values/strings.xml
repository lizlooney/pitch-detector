  <string name="pitch_sharp_note_content_description"><xliff:g id="musical_note">%1$s</xliff:g> sharp, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

  <!-- Content description for a pitch that is sharper than a natural musical note -->
  <string name="pitch_sharper_than_natural_note_content_description"><xliff:g id="half_steps_sharper">%3$s</xliff:g> half steps sharper than <xliff:g id="musical_note">%1$s</xliff:g>, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

