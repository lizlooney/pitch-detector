  <string name="pitch_sharper_than_flat_note_content_description"><xliff:g id="half_steps_sharper">%3$s</xliff:g> half steps sharper than <xliff:g id="musical_note">%1$s</xliff:g> flat, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

