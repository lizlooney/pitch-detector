  <string name="pitch_sharper_than_sharp_note_content_description"><xliff:g id="half_steps_sharper">%3$s</xliff:g> half steps sharper than <xliff:g id="musical_note">%1$s</xliff:g> sharp, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

  <!-- Content description for high pitch [CHAR_LIMIT=NONE] -->
  <string name="pitch_high_content_description">high pitch</string>

