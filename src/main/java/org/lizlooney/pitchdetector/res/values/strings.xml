<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <string name="app_name">PitchDetector</string>
  <string name="Title">Lizzie\'s Pitch Detector</string>

  <!-- Content description for low pitch [CHAR_LIMIT=NONE] -->
  <string name="pitch_low_content_description">low pitch</string>

  <!-- Content description for a pitch that is flatter than a natural musical note -->
  <string name="pitch_flatter_than_natural_note_content_description"><xliff:g id="half_steps_flatter">%3$s</xliff:g> half steps flatter than <xliff:g id="musical_note">%1$s</xliff:g>, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

  <!-- Content description for a pitch that is flatter than a flat musical note -->
  <string name="pitch_flatter_than_flat_note_content_description"><xliff:g id="half_steps_flatter">%3$s</xliff:g> half steps flatter than <xliff:g id="musical_note">%1$s</xliff:g> flat, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

  <!-- Content description for a pitch that is flatter than a sharp musical note -->
  <string name="pitch_flatter_than_sharp_note_content_description"><xliff:g id="half_steps_flatter">%3$s</xliff:g> half steps flatter than <xliff:g id="musical_note">%1$s</xliff:g> sharp, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

  <!-- Content description for a pitch that is equal to a natural musical note -->
  <string name="pitch_natural_note_content_description"><xliff:g id="musical_note">%1$s</xliff:g>, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

  <!-- Content description for a pitch that is equal to a flat musical note -->
  <string name="pitch_flat_note_content_description"><xliff:g id="musical_note">%1$s</xliff:g> flat, octave <xliff:g id="octave_number">%2$s</xliff:g></string>

